{
  "name": "orchestra-system",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "DEBUG=\"\" next dev",
    "build": "prisma generate && next build",
    "start": "next start",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "postinstall": "prisma generate",
    "stress-test": "node scripts/stress-test.js",
    "stress-test:verbose": "node scripts/stress-test.js --verbose",
    "stress-test:no-cleanup": "node scripts/stress-test.js --no-cleanup",
    "stress-test:report": "node scripts/stress-test.js --report=html",
    "stress-test:ts": "ts-node --project scripts/tsconfig.json scripts/stress-test-requests.ts",
    "setup-orchestra": "ts-node --project scripts/tsconfig.json scripts/setup-orchestra-database.ts"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@hookform/resolvers": "^5.1.1",
    "@prisma/client": "^6.10.1",
    "@supabase/supabase-js": "^2.50.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/multer": "^1.4.13",
    "@types/nodemailer": "^6.4.17",
    "bcryptjs": "^3.0.2",
    "dotenv": "^17.0.0",
    "form-data": "^4.0.3",
    "jose": "^6.0.11",
    "lucide-react": "^0.525.0",
    "multer": "^2.0.1",
    "next": "15.3.4",
    "nodemailer": "^7.0.3",
    "prisma": "^6.10.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.58.1",
    "resend": "^4.6.0",
    "zod": "^3.25.67"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.19.1",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.3.4",
    "tailwindcss": "^4",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  }
}
